;;; MSX System Variables
;;; Information from : https://map.grauw.nl/resources/msxsystemvars.php
	
FORCLR:	equ $F3E9		; Foreground Color - 1 byte

BAKCLR:	equ $F3EA		; Background Color - 1 byte

BDCLR:	equ $F3EB 		; Border Color - 1 byte

TXTNAM:	equ $F3B3		; BASE(0) - Screen 0 Name Table - 2 bytes
