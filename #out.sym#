Source: "./main.asm"
                        	    13: 				; ==[ Program ]=============================================
                        	    14: Init:
                        	    15: 	;; Set Screen Mode to 0
00:43D1 CD6C00          	    16: 	call INITXT
                        	    17: 	;; Set Pallette to White on Black
00:43D4 3E01            	    18: 	ld a, 1
00:43D6 32EAF3          	    19: 	ld (BAKCLR), a
00:43D9 32EBF3          	    20: 	ld (BDCLR), a
00:43DC 3E00            	    21: 	ld a, 0
00:43DE 3201C0          	    22: 	ld (CUR_INDEX),a
00:43E1 3202C0          	    23: 	ld (OLD_CUR_INDEX),a
00:43E4 CD6200          	    24: 	call CHGCLR
                        	    25: 	;; Copy the VRAM_BUFFER to RAM so that we can update it
00:43E7 CDFE43          	    26: 	call CopyBufferToRam
                        	    27: 	;; Copy the VRAM_BUFFER in RAM to VRAM
00:43EA CDF043          	    28: 	call CopyWorkBufferToVRAM
00:43ED CD9844          	    29: 	call MainLoop
                        	    30: 
                        	    31: CopyWorkBufferToVRAM:
00:43F0 01C003          	    32: 	ld bc, 960
00:43F3 ED5BB3F3        	    33: 	ld de, (TXTNAM)
00:43F7 2100C1          	    34: 	ld hl, VRM_WRK_AREA
00:43FA CD5C00          	    35: 	call LDIRVM
00:43FD C9              	    36: 	ret
                        	    37: 	
                        	    38: CopyBufferToRam:
00:43FE 211040          	    39: 	ld hl, VRAM_BUFFER
00:4401 1100C1          	    40: 	ld de, VRM_WRK_AREA
00:4404 01C003          	    41: 	ld bc, 960
00:4407 EDB0            	    42: 	ldir
00:4409 C9              	    43: 	ret
                        	    44: 	
                        	    45: IncrementSelector:
                        	    46: 	;; Will need to blank the current selector on the screen before updating the values
00:440A 2A01C0          	    47: 	ld hl, (CUR_INDEX)
00:440D 7D              	    48: 	ld a, l
00:440E 3202C0          	    49: 	ld (OLD_CUR_INDEX),a
00:4411 23              	    50: 	inc hl
00:4412 7D              	    51: 	ld a, l
00:4413 FE15            	    52: 	cp $15
00:4415 CC8544          	    53: 	call z, ZeroSelector
00:4418 3201C0          	    54: 	ld (CUR_INDEX), a
00:441B CD3A44          	    55: 	call UpdateCursor
00:441E CDF043          	    56: 	call CopyWorkBufferToVRAM
00:4421 C9              	    57: 	ret
                        	    58: 
                        	    59: DecrementSelector:
                        	    60: 	;; Will need to Blank the Current Selector On the screen before updating the value
00:4422 2A01C0          	    61: 	ld hl, (CUR_INDEX)
00:4425 7D              	    62: 	ld a, l
00:4426 3202C0          	    63: 	ld (OLD_CUR_INDEX),a
00:4429 2B              	    64: 	dec hl
00:442A 7D              	    65: 	ld a, l
00:442B FEFF            	    66: 	cp $FF
00:442D CC8744          	    67: 	call z, ClampSelector
00:4430 3201C0          	    68: 	ld (CUR_INDEX), a
00:4433 CD3A44          	    69: 	call UpdateCursor
00:4436 CDF043          	    70: 	call CopyWorkBufferToVRAM
00:4439 C9              	    71: 	ret
                        	    72: 
                        	    73: UpdateCursor:
                        	    74: 	;; First we Add the new cursor to the Buffer
                        	    75: 	;; We load the CUR_SEL into de, and multiply it by 40
                        	    76: 	;; to get the row we are on
00:443A 3A01C0          	    77: 	ld a, (CUR_INDEX)
00:443D 67              	    78: 	ld h, a
00:443E 1E28            	    79: 	ld e, 40
00:4440 CD6144          	    80: 	call Mult8x8
                        	    81: 	;; We then add the base address of the Buffer to the HL result
00:4443 1100C1          	    82: 	ld de, VRM_WRK_AREA
00:4446 19              	    83: 	add hl, de
                        	    84: 	;; We finally add a standard offset of 80 to get it started on the right row
00:4447 115000          	    85: 	ld de, 80
00:444A 19              	    86: 	add hl, de
                        	    87: 	;; We then Write the character to the location in RAM
00:444B 36CF            	    88: 	ld (hl), $CF
                        	    89: 	;; We repeat for Clearing the OLD_SEL_INDEX
00:444D 3A02C0          	    90: 	ld a, (OLD_CUR_INDEX)
00:4450 67              	    91: 	ld h, a
00:4451 1E28            	    92: 	ld e, 40
00:4453 CD6144          	    93: 	call Mult8x8
00:4456 1100C1          	    94: 	ld de, VRM_WRK_AREA
00:4459 19              	    95: 	add hl, de
00:445A 115000          	    96: 	ld de, 80
00:445D 19              	    97: 	add hl, de
00:445E 3620            	    98: 	ld (hl), $20
00:4460 C9              	    99: 	ret
                        	   100: 
                        	   101: ;;; Mult h by e and place in hl
                        	   102: Mult8x8:
00:4461 1600            	   103: 	ld d, 0
00:4463 CB24            	   104: 	sla h
00:4465 9F              	   105: 	sbc a, a
00:4466 A3              	   106: 	and e
00:4467 6F              	   107: 	ld l,a
00:4468 0607            	   108: 	ld b, 7
                        	   109: Mult8_8_loop:
00:446A 29              	   110: 	add hl, hl
00:446B 3001            	   111: 	jr nc, $+3
00:446D 19              	   112: 	add hl, de
00:446E 10FA            	   113: 	djnz Mult8_8_loop
00:4470 C9              	   114: 	ret
                        	   115: ;;; Mult DE by A and store Result in AHL
                        	   116: Mult8x16:
00:4471 0E00            	   117: 	ld c, 0
00:4473 61              	   118: 	ld h, c
00:4474 6C              	   119: 	ld l, h
00:4475 87              	   120: 	add a, a
00:4476 3002            	   121: 	jr nc, $+4
00:4478 62              	   122: 	ld h,d
00:4479 6B              	   123: 	ld l,e
00:447A 0607            	   124: 	ld b,7
                        	   125: Mult8_16_loop:
00:447C 29              	   126: 	add hl, hl
00:447D 17              	   127: 	rla
00:447E 3002            	   128: 	jr nc, $+4
00:4480 19              	   129: 	add hl, de
00:4481 89              	   130: 	adc a, c
00:4482 10F8            	   131: 	djnz Mult8_16_loop
00:4484 C9              	   132: 	ret
                        	   133: 	
                        	   134: ZeroSelector:
00:4485 AF              	   135: 	xor a
00:4486 C9              	   136: 	ret
                        	   137: 
                        	   138: ClampSelector:
00:4487 3E14            	   139: 	ld a, $14
00:4489 C9              	   140: 	ret
                        	   141: 	
                        	   142: CheckInput:
00:448A CD9F00          	   143: 	call CHGET
00:448D FE1E            	   144: 	cp 30
00:448F CC2244          	   145: 	call z, DecrementSelector
00:4492 FE1F            	   146: 	cp 31
00:4494 CC0A44          	   147: 	call z, IncrementSelector
00:4497 C9              	   148: 	ret
                        	   149: 
                        	   150: MainLoop:
00:4498 F3              	   151: 	di
00:4499 CD8A44          	   152: 	call CheckInput
00:449C 18FA            	   153: 	jr MainLoop
                        	   154: 		
